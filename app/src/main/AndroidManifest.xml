        </activity>
        <activity android:name=".First_Question" />
        <activity android:name=".Second_Question" />
        <activity android:name=".Third_Question" />
        <activity android:name=".Fourth_Question" />
        <activity android:name=".Sixth_Question" />
        <activity android:name=".Seventh_Question" />
        <activity android:name=".Eightth_Question" />
        <activity android:name=".Fifth_Question" />
        <activity android:name=".Ninth_Question" />
        <activity android:name=".Tenth_Question" />
        <activity android:name=".Eleventh_Question" />
        <activity android:name=".Twelveth_Question" />
        <activity android:name=".Thirteenth_Question" />
        <activity android:name=".Fourteenth_Question" />
        <activity android:name=".Fifthteenth_Question" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>